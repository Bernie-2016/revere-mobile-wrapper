{
  "name": "Revere Mobile Wrapper",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "Modify Revere Mobile App CSS",
  "permissions": [
    "*://mobile.reverehq.com/*"
  ],
  "content_scripts": 
  [
    {
      "matches": ["*://mobile.reverehq.com/*"],
      "css": ["css/main.css"],
      "js": ["js/vendor/jquery.min.js", "js/main.js"],
      "run_at": "document_start"
    }
  ]
}
